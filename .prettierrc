{
	"printWidth": 100,
	"tabWidth": 2,
	"useTabs": true,
	"semi": false,
	"singleQuote": true,
	"quoteProps": "preserve",
	"jsxSingleQuote": true,
	"trailingComma": "es5",
	"bracketSpacing": true,
